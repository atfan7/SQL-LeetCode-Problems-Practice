Problem:1741

select event_day as day, emp_id, sum(out_time - in_time) as total_time 


from 
Employees 

group by emp_id, event_day

Problem: 1757

select product_id

from 
Products 

where low_fats='Y' and recyclable ='Y'


Problem: 1789

select employee_id, department_id

from 
Employee
where 
primary_flag='Y'

Union 

select employee_id, department_id
from Employee
group by employee_id
having 
count(employee_id)=1

Problem 1795

select 

product_id,

'store1' as store,

store1 as price
from Products
where store1 is not null
union
select 

product_id,

'store2' as store,

store2 as price
from Products
where store2 is not null
union
select 

product_id,

'store3' as store,

store3 as price
from Products
where store3 is not null
