PROBLEM 1809 :::

select 
session_id
from playback where session_id not in (

select 
p.session_id

from 
Playback p left join Ads a
on p.customer_id= a.customer_id 
where a.timestamp between p.start_time and p.end_time)

PROBLEM 1821::

select

customer_id
from 

Customers
where year ='2021' 
group by customer_id
having sum(revenue)>0
